{
  "name": "kdk-web",
  "version": "1.1.3",
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.15",
    "@internal/graphql": "file:src/graphql",
    "antd": "3.21.4",
    "apollo-boost": "0.4.4",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client-preset": "1.0.8",
    "apollo-link-context": "1.0.18",
    "apollo-link-error": "1.1.11",
    "apollo-link-http": "1.5.15",
    "axios": "0.18.1",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "graphql": "14.4.2",
    "graphql.macro": "1.4.2",
    "i18next": "14.1.1",
    "immutability-helper": "2.9.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "path-to-regexp": "3.0.0",
    "query-string": "6.8.2",
    "react": "16.9.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-apollo": "2.5.8",
    "react-dom": "16.9.0",
    "react-dropzone": "8.2.0",
    "react-i18next": "9.0.10",
    "react-native-vector-icons": "6.6.0",
    "react-pose": "4.0.8",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-breadcrumbs-hoc": "2.3.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.8",
    "react-sizes": "1.0.4",
    "react-sortable-hoc": "1.9.1",
    "recompose": "0.30.0",
    "redux": "3.7.2",
    "redux-persist": "5.10.0",
    "styled-components": "3.4.10"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "clean:server": "rm -rf ../backend/dist/client && rm -rf ../backend/src/client",
    "copy": "$npm_execpath run clean:server && cp -R build/ ../backend/src/client && cp -R build/ ../backend/dist/client",
    "build:copy": "$npm_execpath run build && $npm_execpath run copy",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clear:internal": "rm -rf node_module/@internal && $npm_execpath install",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://backend:4000",
  "devDependencies": {
    "@babel/helper-module-imports": "7.0.0",
    "babel-plugin-emotion": "10.0.15",
    "babel-plugin-import": "1.12.0",
    "babel-plugin-import-graphql": "2.7.0",
    "customize-cra": "0.5.0",
    "graphql-tag.macro": "2.1.0",
    "react-app-rewire-less": "2.1.3",
    "react-app-rewired": "2.1.3"
  },
  "apollo": {
    "schemas": {
      "vsm": {
        "endpoint": "http://localhost:4000/graphql",
        "headers": {}
      }
    }
  }
}
