    "react": "16.9.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-apollo": "2.4.1",
    "react-dom": "16.9.0",
    "react-dropzone": "8.2.0",
    "react-i18next": "9.0.10",
    "react-native-vector-icons": "6.2.0",
    "react-pose": "4.0.8",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-breadcrumbs-hoc": "2.3.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.1.1",
    "react-sizes": "1.0.4",
    "react-sortable-hoc": "1.9.1",
    "react-use": "12.2.3",
    "recompose": "0.30.0",
    "redux": "3.7.2",
    "redux-persist": "5.10.0",
    "styled-components": "4.2.0",
    "subscriptions-transport-ws": "0.9.16"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "clean:server": "rm -rf ../backend/dist/client && rm -rf ../backend/src/client",
    "copy": "$npm_execpath run clean:server && cp -R build/ ../backend/src/client && cp -R build/ ../backend/dist/client",
    "build:copy": "$npm_execpath run build && $npm_execpath run copy",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clear:internal": "rm -rf node_module/@internal && $npm_execpath install",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "generate": "graphql-codegen"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://backend:4000",
  "devDependencies": {
    "@babel/helper-module-imports": "7.0.0",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/fragment-matcher": "^1.12.2",
    "babel-plugin-emotion": "10.0.15",
    "babel-plugin-import": "1.11.0",
    "babel-plugin-import-graphql": "2.6.2",
    "customize-cra": "0.2.11",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "5.0.1",
    "eslint-plugin-flowtype": "3.12.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "graphql-tag.macro": "2.1.0",
    "prettier": "1.18.2",
    "react-app-rewire-less": "2.0.3",
    "react-app-rewired": "2.0.3"
  },
  "apollo": {
    "schemas": {
      "vsm": {
        "endpoint": "http://localhost:4000/graphql",
        "headers": {}
      }
    }
  }
}
