{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "plugins": ["prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "interface-name": false
  },
  "jsRules": {}
}