{
  "name": "bvdk-wertung",
  "version": "1.1.10",
  "description": "",
  "main": "src/start.ts",
  "bin": "dist/start.js",
  "scripts": {
    "watch": "nodemon",
    "start": "ts-node src/start.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "cp package.json src/package.json && tsc",
    "deploy:electron": "rimraf ./../kdk-electron/resources/app && cp -R dist ./../kdk-electron/resources/app",
    "build:deploy:electron": "npm version patch && yarn run build && yarn run deploy:electron",
    "version": "npm version patch",
    "dev": "npm-run-all --parallel start playground",
    "playground": "graphql playground",
    "kill:port": "kill-port 4000",
    "version:patch": "npm version patch",
    "prisma:migrate:help": "prisma migrate up --help",
    "prisma:create:migration": "prisma migrate save --name \"migration\" --experimental",
    "prisma:migrate": "prisma migrate up --experimental",
    "prisma:generate:client": "prisma generate",
    "prisma": "export DATABASE_URL=postgresql://bvdk:bvdkpw@localhost:5432/bvdk?schema=public && npm run prisma:create:migration && npm run prisma:migrate && npm run prisma:generate:client",
    "prisma:prod": "npm run prisma:create:migration && npm run prisma:migrate && npm run prisma:generate:client"
  },
  "keywords": [],
  "author": "Marcel RÃ¶sler",
  "license": "ISC",
  "dependencies": {
    "@prisma/cli": "^2.0.0-beta.2",
    "@prisma/client": "^2.0.0-preview022",
    "csvtojson": "2.0.10",
    "express": "4.17.1",
    "express-fileupload": "1.1.7-alpha.3",
    "graphql-subscriptions": "1.1.0",
    "graphql-type-json": "0.3.1",
    "graphql-yoga": "1.18.3",
    "json2csv": "5.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "lodash-advanced": "1.0.4",
    "lodash-id": "0.14.0",
    "lowdb": "1.0.0",
    "moment": "2.24.0",
    "passport": "0.4.1",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pdfmake": "0.1.65",
    "reflect-metadata": "0.1.13",
    "type-graphql": "0.17.6"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/express-fileupload": "^1.1.3",
    "@types/jest": "25.2.1",
    "@types/json2csv": "^5.0.0",
    "@types/lodash": "4.14.150",
    "@types/passport": "^1.0.3",
    "graphql-cli": "3.0.14",
    "jest": "25.3.0",
    "nodemon": "2.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.4",
    "ts-jest": "25.4.0",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "graphql": "14.1.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|tmp|node_modules)/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node12"
    ],
    "output": "pkg"
  }
}
